# Example environment variables for Get2KnowMe
# Copy this file to .env and fill in your own values for local development

# MongoDB connection string (use a local/test database for development)
MONGODB_URI=mongodb://localhost:27017/get2knowme_dev

# JWT secret for signing authentication tokens
JWT_SECRET=your_jwt_secret_here

# Node environment (development or production)
NODE_ENV=development

# Port for the backend server
PORT=3001

# Frontend URL for CORS (adjust as needed)
CLIENT_URL=http://localhost:5173

# Mongoose Field Encryption Key (for encrypting sensitive fields in MongoDB)
FIELD_ENCRYPTION_SECRET=your_field_encryption_secret_here

